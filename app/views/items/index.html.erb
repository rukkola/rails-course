<h2>All Item</h2>

<% @items.each do |item| %>
  <ul>
 <!--   <li><%= link_to item.name, "/items/#{item.id}" %></li>
    Или вариант ниже
 -->
    <li><%= link_to item.name, item_path(item) %></li>
    <li><%= item.price %></li>
    <li><%= item.desc %></li>
    <li><%= button_to "delete", item_path(item), method: :delete %></li>
   </ul>
<% end %>

<br/>

<%= link_to 'new item', new_item_path %>


